# --------------------------

# dotfiles
# --------------------------
export DOTFILES="${HOME}/.dotfiles"

# gdrive
# --------------------------
export GDRIVE="${HOME}/gdrive"

# zsh and .oh-my-zsh
ZSH_THEME="agnoster"
plugins=(git)
export ZSH="$HOME/.oh-my-zsh"

# common sources
